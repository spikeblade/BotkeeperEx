{
  "name": "botkeeperex",
  "version": "1.0.0",
  "description": "Test app ",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "start": "npm run build && node ./build/app.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/build/tests/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spikeblade/BotkeeperEx.git"
  },
  "author": "Cesar Arbelaez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spikeblade/BotkeeperEx/issues"
  },
  "homepage": "https://github.com/spikeblade/BotkeeperEx#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "ts-jest": "^29.2.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.5.3"
  }
}
